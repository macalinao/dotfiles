DOTFILES=$HOME/dotfiles
source $DOTFILES/lib/shell/zsh.sh

for UTIL in $(ls $DOTFILES/util/*.sh); do
  source $UTIL
done

PATH="$HOME/.local/bin:$PATH"

source $DOTFILES/lib/transfer.sh
source $DOTFILES/lib/editor.sh
source $DOTFILES/lib/lang/python.sh
source $DOTFILES/lib/lang/go.sh
source $DOTFILES/lib/lang/ruby.sh
source $DOTFILES/lib/lang/scala.sh
